<div class="row">

    <div id="add-question-warning-container" class="alert alert-warning alert-dismissible fade show" role="alert"
      style="<% if(result_code != 0){ %> display: none; <% } %>">
      <span id="add-question-warning">Та дахин оролдоно уу</span>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>

    <div class="col-lg-12">
      <div id="question-list-container" class="list-group">
        <% if(questionsData != null){ %>
          <% for (var i = 0; i < questionsData.length; i++) { %>
            <a href="/questions/<%= questionsData[i]._id %>" class="list-group-item list-group-item-action flex-column align-items-start">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1"><%= questionsData[i].text %></h5>
                <small><%= timeagoInstance.format(questionsData[i].createdDate, 'mn') %></small>
              </div>
              <!-- <p class="mb-1"></p> -->
              <small id="question-date"><%= questionsData[i].userId %></small>
            </a>
          <% } %>

        <% } %>

        </div>
    </div>
</div>
